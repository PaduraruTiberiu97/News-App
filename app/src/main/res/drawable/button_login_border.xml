<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="@integer/config_navAnimTime">
    <item android:state_pressed="false">
        <shape>
            <stroke android:width="1dp" android:color="#AEAEAE" />
            <solid android:color="@color/colorWhite" />
            <corners android:radius="3dp" />

        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="#AEAEAE" />
            <solid android:color="@color/grey" />
            <corners android:radius="3dp" />

        </shape>
    </item>

</selector>